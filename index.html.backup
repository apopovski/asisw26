		<script>
			const buttons = document.querySelectorAll(".nav button");
			const sections = document.querySelectorAll("[data-day-section]");

			const setActiveDay = (day) => {
				buttons.forEach((button) => {
					button.classList.toggle("active", button.dataset.day === day);
				});
				sections.forEach((section) => {
					section.style.display = section.id === day ? "block" : "none";
				});
			};

			buttons.forEach((button) => {
				button.addEventListener("click", () => {
					setActiveDay(button.dataset.day);
				});
			});

			setActiveDay("thursday");
		</script>
